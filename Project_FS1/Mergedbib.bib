@Article{         mignard-henon-1984,
  title         = {About an unsuspected integrable problem},
  author        = {Mignard, F. and H\'enon, M.},
  AFFILIATION   = {AA(Centre d'Etudes et de Recherches Geodynamiques et
                   Astronomiques, Grasse, Alpes-Maritimes, France)
                   AB(Nice, Observatoire, Nice, France)},
  journal       = {\CelMech},
  volume        = {33},
  pages         = {239--250},
  year          = {1984},
  KEYWORDS      = {CELESTIAL MECHANICS, GRAVITATIONAL EFFECTS,
                   ORBIT CALCULATION, PARTICLE MOTION,
                   PLANETARY GRAVITATION, DIFFERENTIAL EQUATIONS,
                   NUMERICAL INTEGRATION, SECULAR VARIATIONS,
                   TRANSFORMATIONS (MATHEMATICS)},
  BIBCODE       = {1984CeMec..33..239M},
  ABSTRACT      = {
The motion of a particle orbiting a planet is investigated when both
the gravitational attraction of the planet and the radial radiation
force due to the sun are taken into account. Because of this latter
force, a rotating frame is introduced so as to maintain the sun at
fixed coordinates. It is established that the secular problem, which
has two degrees of freedom, is completely integrable and that it
possesses only periodical solutions.  An explicit analytical solution
is provided.},
  topic         = {MPG},
  copy          = {private copy}
}

