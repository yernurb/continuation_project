@Article{         prakash-1975,
  title         = {Magnetospheric protons and electrons encountered by
                    the moon in the plasma sheet},
  author        = {Prakash, A.},
  AFFILIATION   = {AA(Cornell University, Ithaca, N.Y.)},
  publisher     = {Lunar Science Institute},
  note          = {Conference on Interactions
