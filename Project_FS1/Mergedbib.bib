@Article{         humes-1980,
  title         = {Results of {Pioneer~10} and 11 meteoroid experiments~---
                  {Interplanetary} and near-{Saturn}},
  author        = {Humes, D. H.},
  AFFILIATION   = {AA(NASA, Langley Research Center, Hampton, Va.)},
  journal       = {\JGR},
  volume        = {85},
  pages         = {5841--5852},
  year          = {1980},
  KEYWORDS      = {ASTEROID BELTS, INTERPLANETARY SPACE, METEOROIDS,
                   PIONEER 10 SPACE PROBE, PIONEER 11 SPACE PROBE,
                   SATURN ATMOSPHERE, OPTICAL THICKNESS,
