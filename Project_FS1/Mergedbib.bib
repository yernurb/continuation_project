@Misc{            mandeville-1991,
  title         = {Study of meteoroid impact craters on various
                    materials {(AO 138-1)}. {Attempt} at dust debris
                    collection with stacked detectors {(AO 138-2)}},
  author        = {Mandeville, J. C.},
