@Phdthesis{       lodhi-1987,
  title         = {Conditions for geomagnetically trapping sub-micron
                  lunar ejecta between $L = 1.2$ and $L = 3.1$},
  author        = {Lodhi, L. M.},
  AFFILIATION   = {Baylor Univ., Houston, TX},
