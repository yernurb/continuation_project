@Inproceedings{   humes-1976,
  title         = {The {Jovian} meteoroid environment},
  author        = {Humes, D. H.},
  AFFILIATION   = {AA(NASA, Langley Research Center, Hampton, Va.)},
  booktitle     = {Jupiter: Studies of the interior, atmosphere,
                   magnetosphere and satellites; Proceedings of the
