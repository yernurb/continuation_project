@Inproceedings{   gruen-et-al-1985b,
  title         = {Orbits of interplanetary dust particles inside
                  {1~AU} as observed by {Helios}},
  author        = {Gr\"un, H. and Fechtig, H. and Kissel, J.},
  AFFILIATION   = {AC(Max-Planck-Institut fuer Kernphysik, Heidelberg,
                   West Germany)},
  booktitle     = {Properties and interactions of interplanetary
                   dust; Proceedings of the Eighty-fifth Colloquium,
                   Marseille, France, July 9-12, 1984 (A86-42326 20-90)},
  publisher     = {Dordrecht, D. Reidel Publishing Co.},
  year          = {1985},
  pages         = {105--111},
  KEYWORDS      = {HELIOS PROJECT, INTERPLANETARY DUST, PARTICLE MOTION,
                   SOLAR ORBITS, AZIMUTH, FLUX QUANTIZATION, IMPACT,
                   MICROPARTICLES, PROBABILITY DISTRIBUTION FUNCTIONS},
  BIBCODE       = {1985piid.......105G},
  ABSTRACT      = {
The in situ dust detectors on board the Helios spaceprobes detected
impacts of micrometeoroids between 0.3 AU and 1 AU distance from the
sun. Among the measured quantities were mass, impact speed and flight
direction of the dust particles. Radial variations of the flux,
azimuthal distribution, and impact speed are discussed. Significantly
different results were obtained for the 'apex' particles observed by
both ecliptic and south sensors and for the 'eccentric' particles
detected only by the south sensor. The radial spatial density
variation of the 'eccentric' particles is compatible with that
derived for zodiacal light particles. Whereas the spatial density for
'apex' particles peaks at 0.5 to 0.6 AU. From the measured quantities
probability distributions of orbital elements were derived for the
observed micrometeoroids. About 60 percent of the observed
micrometeroids ('apex' particles) are on low energy orbits (a less
than about 0.6 AU). About 30 percent of the particles ('eccentric'
particles) had high energy orbits (a greater than about 0.9 AU). More
than 10 percent of the observed particles show high probabilities for
travelling on hyperbolic orbits.},
  topic         = {ZODIAC}
}

