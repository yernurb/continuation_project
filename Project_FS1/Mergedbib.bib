@Article{         bertaux-blamont-1976,
  title         = {Possible evidence for penetration of interstellar
                    dust into the solar system},
  author        = {Bertaux, J. L. and Blamont, J. E.},
  AFFILIATION   = {AB(CNRS, Service d'Aeronomie, Verrieres-le-Buisson,
                    Essonne, France)},
  journal       = {Nature},
  volume        = {262},
  date          = {July 22},
  year          = {1976},
  pages         = {263--266},
  KEYWORDS      = {COSMIC DUST, INTERSTELLAR MATTER, LIGHT SCATTERING,
                    PARTICLE INTERACTIONS, SOLAR SYSTEM, ASTRONOMICAL
                    MODELS, GRAVITATIONAL EFFECTS, MICROMETEOROIDS,
                    ZODIACAL DUST, ZODIACAL LIGHT},
  ABSTRACT      = {
Because of the motion of the sun through the surrounding interstellar
medium, interstellar dust particles may penetrate deep into the solar
system. In this paper, the conditions for this penetration are
investigated, and it is found that the distribution of such particles
should peak along the line of gravitational focusing. Two possible
consequences are compared with experimental results: the rate at which
particles should strike a micrometeoroid detector and the distribution of
scattered solar light. The number density of particles penetrating the
solar system, derived from this comparison, is found to be much smaller
than predicted by a crude model of interstellar matter.},
  topic         = {INTERSTELLAR}
}

