@Phdthesis{       neste-1975,
  title         = {An experimental model of the asteroid/meteoroid
                  environment from 1.0 to 3.5~{AU}:
		  {Its} characteristics and implications},
  author        = {Neste, S. L.},
  AFFILIATION   = {Drexel Univ., Philadelphia, PA},
  school        = {Drexel Univ., Philadelphia, PA},
  year          = {1975},
  KEYWORDS      = {ASTEROIDS, ENVIRONMENT MODELS, METEOROIDS,
                   SPATIAL DISTRIBUTION, DETECTORS,
                   EQUIPMENT SPECIFICATIONS, PARTICLE SIZE DISTRIBUTION,
                   PARTICULATES, PIONEER 10 SPACE PROBE},
  BIBCODE       = {1975PhDT.........1N},
  ABSTRACT      = {
A brief historical summary of the techniques and instrumentation
employed to investigate the interplanetary particulate environment is
given. The environmental models which resulted from these
investigations are presented to provide a basis for discussing the
results of measurements made from the Pioneer 10 spacecraft by the
asteroid meteoroid detector (AMD). The AMD detects and measures
interplanetary particles by sensing the sunlight reflected from them
as they pass through the instrument's overlapping optical
fields-of-view. The concepts and engineering characteristics of the
AMD necessary to understand the analysis of the measurements are
presented. The results and conclusions of the analysis are based on
the first in situ measurements of this type conducted in the
interplanetary region beyond the orbit of Mars. The spatial
concentration and size distribution for particulates measured by the
asteroid/meteoroid detector on Pioneer 10 between 1.0 and 3.5
astronomical units are presented.},
  topic         = {ZODIAC}
}

