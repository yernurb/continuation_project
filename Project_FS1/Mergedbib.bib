@Inproceedings{         clube-1984,
  title         = {The origin and evolution of comets},
  author        = {Clube, S. V. M.},
  AFFILIATION   = {AA(Royal Observatory, Edinburgh, Scotland)},
  booktitle     = {Asteroids, comets, meteors; Proceedings of the
                   Meeting, Uppsala, Sweden, June 20-22, 1983 (A85-26851
                   11-89).},
  publisher     = {Uppsala, Sweden, Astronomiska Observatoriet},
