@Article{         horanyi-mendis-1986,
  title         = {The dynamics of charged dust in the tail of Comet
                   {Giacobini-Zinner}},
  author        = {Hor\'anyi, M. and Mendis, D. A.},
  AFFILIATION   = {AA(Florida State University, Tallahassee; Magyar
                   Tudomanyos Akademia, Kozponti Fizikai Kutato Intezet,
                   Budapest, Hungary) AB(California, University, La
                   Jolla)},
  journal       = {\JGR},
  volume        = {91},
  year          = {1986},
  pages         = {355--361},
  KEYWORDS      = {CHARGED PARTICLES, COMET NUCLEI, COMET TAILS,
                   DIGITAL SIMULATION, GIACOBINI-ZINNER COMET,
                   INTERPLANETARY DUST, INTERPLANETARY MAGNETIC FIELDS,
                   NASA SPACE PROGRAMS, PARTICLE SIZE DISTRIBUTION,
                   SPACECRAFT ORBITS},
  BIBCODE       = {1986JGR....91..355H},
  ABSTRACT      = {
A numerical simulation has been performed to calculate the
distributions of dust particles of various sizes down the tail of
comet P/Giacobini-Zinner. When the electrostatic charging of the
grains in the plasma and radiative environment of the comet is taken
into account, it is found that the distribution of the grains
(particularly at the lowest end of the mass spectrum) in a plane
normal to the orbital plane is entirely different from what is
expected had the grains been uncharged. Although the NASA-ICE
spacecraft, which will fly through the tail of this comet almost
normal to its orbital plane on September 11, 1985, has no dedicated
dust experiments, it is expected that the plasma wave instrument will
serve as an indirect detector, even of the smallest grains, via the
plasma clouds created by the high-velocity dust impacts. Knowledge of
the spatial variation of the grain sizes encountered along the flight
path of the spacecraft will provide us with the information necessary
to calculate the electrostatic potential of the grains, which in turn
will lead to an estimation of the role of the electromagnetic forces
on the dynamics of such grains.},
  topic         = {COMETS}
}

