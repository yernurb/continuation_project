@Article{         simpson-et-al-1980,
  title         = {Saturnian trapped radiation and its absorption by
                   satellites and rings~---
		   {The} first results from {Pioneer}~11},
  author        = {Simpson, J. A. and Bastian, T. S. and Chenette, D. L. and
                   Lentz, G. A. and McKibben, R. B. and Pyle, K. R. and
                   Tuzzolino, A. J.},
  AFFILIATION   = {AG(Chicago, University, Chicago, Ill.)},
  journal       = {Science},
  volume        = {207},
  date          = {Jan. 25},
  year          = {1980},
  pages         = {411--415},
  KEYWORDS      = {NATURAL SATELLITES, PIONEER 11 SPACE PROBE,
                   RADIATION ABSORPTION, RADIATION BELTS,
                   SATURN (PLANET), SATURN RINGS, CHARGE DISTRIBUTION,
                   CHARGED PARTICLES, ELECTRON DISTRIBUTION,
