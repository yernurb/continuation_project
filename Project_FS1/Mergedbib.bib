@Article{         hughes-mcbride-1990,
  title          = {The spatial density of large meteoroids
                   in the inner solar system},
  author        = {Hughes, D. W. and {MacBride}, N.},
  AFFILIATION   = {AB(Sheffield, University, England)},
  journal       = {\MNRAS},
  volume        = {243},
