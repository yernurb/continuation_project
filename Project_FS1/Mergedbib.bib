@Article{         sicardy-et-al-1991,
  title         = {Neptune's rings, 1983-1989: {Ground}-based stellar
                  occultation observations. {I}~--- {Ring}-like arc
                  detections},
  author        = {Sicardy, B. and Brahic, A. and Roques, F.},
  AFFILIATION   = {AB(Paris VII, Universite; Paris, Observatoire,
                    Meudon, France) AC(Paris, Observatoire, Meudon,
                    France)},
  journal       = {Icarus},
  volume        = {89},
  pages         = {220--243},
  year          = {1991},
  KEYWORDS      = {NEPTUNE (PLANET), PLANETARY RINGS, SPACE
                    OBSERVATIONS (FROM EARTH), STELLAR OCCULTATION,
                    ASTROMETRY, ASTRONOMICAL MODELS, LIGHT CURVE,
                    TELESCOPES, VOYAGER 2 SPACECRAFT},
  BIBCODE       = {1991Icar...89..220S},
  ABSTRACT      = {
The ground-based stellar occultation observations presently analyzed
furnish evidence for either incomplete or highly inhomogeneous
ring-like material around Neptune, as well as an upper limit for the
density of continuous rings of the planet. The analysis shows that
the June 22, 1984 and August 20, 1985 observations of material around
Neptune are probably due to highly inhomogeneous ring-like material,
or arcs, orbiting the planet. The extant data are also studied for
narrow (10-50 km) continuous rings; no evidence is found for them
between 1.1 and 3.5 Neptunian radii; neither are there diffuse rings
near the 53,200 and 63,000 km radii where Voyager 2 observed two
continuous rings.},
  topic         = {NEPTUNE}
}

