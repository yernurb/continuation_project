@Inproceedings{   hyde-alexander-1989,
  title         = {Transport dynamics calculated under the full {Mie}
                  scattering theory for micron and submicron lunar
                  ejecta in selenocentric, cislunar, and geocentric
                  space},
  author        = {Hyde, T. W. and Alexander, W. M.},
  AFFILIATION   = {AB(Baylor University, Waco, TX)},
  booktitle     = {Lunar and Planetary Science Conference, 19th,
                  Houston, TX, Mar. 14-18, 1988, Proceedings},
  publisher     = {Cambridge/Houston, TX, Cambridge
