@Article{         artymowicz-et-al-1989,
  author        = { {Artymowicz}, P. and {Burrows}, C. and {Paresce}, F.},
  title         = { The structure of the {Beta} {Pictoris} circumstellar
                  disk from combined {IRAS} and coronagraphic observations },
  journal       = {\ApJ },
  year          = 1989,
  volume        = 337,
  pages         = "494--513",
  ABSTRACT      = {
 A grid of models of the particulate disk around Beta Pic is constructed
in which the same grains orbiting the star give rise to both the
directly observable scattered starlight and the FIR thermal radiation
measured by IRAS. The spatial distribution of particles in the disk is
determined to find a distribution from coronograph images (Paresce and
Burrows, 1987) and used with IRAS measurements to find a distribution of
matter in both the directly observable and occulted disk regions. An
upper limit for the tilt between the disk ecliptic and the line of sight
is determined to be 14 deg, corresponding to an infinitesimally thin
disk. Also, the normal optical thickness of the disk, the grain radii,
and the model-dependent geometric albedo of grains in the visible are
determined. Possible grain composition is also considered.},
  topic         = {STARS},
  copy          = {private copy}
}

