@Article{         schneider-1975,
  title         = {Impact ejecta exceeding lunar escape velocity},
  author        = {Schneider, E.},
  AFFILIATION   = {AA(Max-Planck-Institut fuer Kernphysik, Heidelberg,
                    West Germany)},
  publisher     = {Lunar Science Institute},
  note          = {Symposium on Origin and
