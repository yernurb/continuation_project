@Article{         fechtig-1984,
  title         = {The interplanetary dust environment beyond 1 AU and in
                  the vicinity of the ringed planets},
  author        = {Fechtig, H.},
  AFFILIATION   = {AA(Max-Planck-Institut fuer Kernphysik, Heidelberg,
                   West Germany)},
  journal       = {\AdvSR},
  volume        = {4},
  number        = {9},
  year          = {1984},
  pages         = {5--11},
  KEYWORDS      = {INTERPLANETARY DUST, JUPITER RINGS,
                   PARTICLE DENSITY (CONCENTRATION), SATURN RINGS,
                   URANUS RINGS, COMETS, HELIOS SATELLITES,
                   ORGANIC MATERIALS, PARTICLE SIZE DISTRIBUTION,
                   PENETRATION, PIONEER 10 SPACE PROBE,
                   PIONEER 11 SPACE PROBE, PLANETARY STRUCTURE,
                   SPACEBORNE EXPERIMENTS},
  BIBCODE       = {1984dsc.....4....5F},
  ABSTRACT      = {
The results of Pioneer 10/11 observations of the interplanetary dust
environment beyond 1 AU, and near the ringed planets are reviewed. It
is suggested that the cometary core/mantle grain model of Greenberg
(1983) can account for the apparent contradiction between the results
of two Pioneer experiments: the penetration experiment; and the
optical experiment. It is also suggested that the observed
enhancements of the interplanetary dust flux by 1-2 orders of
