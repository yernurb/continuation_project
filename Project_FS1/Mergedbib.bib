@Article{         johnson-et-al-1980,
  title         = {Dust in {Jupiter's} magnetosphere~--- {An}~{Io} source},
  author        = {Johnson, T. V. and Morfill, G. and Gr\"un, E.},
  AFFILIATION   = {AA(California Institute of Technology, Jet
                  Propulsion Laboratory, Pasadena, Calif.)
                  AC(Max-Planck-Institut fuer Kernphysik, Heidelberg,
                  West Germany)},
  journal       = {\GRL},
  volume        = {7},
  year          = {1980},
  pages         = {305--308},
  KEYWORDS      = {INTERPLANETARY DUST, IO, JUPITER ATMOSPHERE,
                    PARTICLE INTERACTIONS, PLANETARY MAGNETOSPHERES,
                    CHARGED PARTICLES, COULOMB COLLISIONS, JUPITER
                    RINGS, LORENTZ FORCE, PLANETARY GRAVITATION,
                    PLASMASPHERE},
  BIBCODE       = {1980GeoRL...7..305J},
  ABSTRACT      = {
The possibility of removing particles from Io by interactions with the
Jovian magnetosphere has been investigated. It is found that dust grains
of about 0.1 micron radius will rapidly become charged if exposed to the
ambient Jovian plasma. For particles this size in Io's volcanic plumes,
the Lorentz force can overcome Io's gravity and these particles can
escape. Escaping dust will be controlled by the topology of the
magnetosphere and it is suggested that Io-derived dust may be an
important source of erosive impacts on large Jovian ring particles and
destructive collisions with small particles. The ring particles
themselves will interact with the Jovian plasma and it is suggested that
Coulomb scattering of plasma particles by charged dust grains may
produce a plasma gap or void in the vicinity of the rings.},
  topic         = {JUPITER}
}

