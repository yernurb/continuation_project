@Article{         humes-et-al-1980,
  title         = {Impact of {Saturn} ring particles on {Pioneer~11}},
  author        = {Humes, D. H. and Oneal, R. L. and Kinard, W. H. and
                   Alvarez, J. M.},
  AFFILIATION   = {AD(NASA, Langley Research Center, Hampton, Va.)},
  journal       = {Science},
  volume        = {207},
