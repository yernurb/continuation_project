@Article{         rabinowitz-et-al-1993,
  title         = {Evidence for a near-{Earth} asteroid belt},
  author        = {Rabinowitz, D. L. and Gehrels, T. and Scotti, J. V. and
                    McMillan, R. S. and Perry, M. L. and Wisniewski, W. and
                    Larson, S. M. and Howell, E. S. and M\"uller, B. E. A.},
