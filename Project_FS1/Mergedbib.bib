@Article{         paolicci-et-al-1982,
  title         = {High-velocity impact experiments needed to improve our
                   understanding of the asteroids},
  author        = {Paolicchi, P. and Farinella, P. and Zappala, V.},
  AFFILIATION   = {AB(Osservatorio Astronomico di Brera, Merate, Italy)
