@Inbook{          fernandez-ip-1991,
  title         = {Statistical and evolutionary aspects of cometary
                   orbits},
  author        = {Fernandez, J. A. and Ip, W.-H.},
  AFFILIATION   = {AA(Univ. de la Republica, Montevideo, Uruguay)
                   AB(Max-Planck-Inst. fuer Aeronomie,
