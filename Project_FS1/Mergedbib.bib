@Inproceedings{   dohnanyi-1977,
  title         = {Groups of micrometeoroids in the earth-moon system},
  author        = {Dohnanyi, J. S.},
  AFFILIATION   = {AA(Max-Planck-Institut fuer Kernphysik, Heidelberg,
                    West Germany)},
  booktitle     = {Space research XVII; Proceedings of the Open
                    Meetings of Working Groups on Physical Sciences,
                    June 8-19, 1976 and Symposium on Minor Constituents
                    and Excited Species, Philadelphia, Pa., June 9, 10,
                    1976},
  publisher     = {Oxford and New York, Pergamon Press},
  pages         = {623--625},
  year          = {1977},
  KEYWORDS      = {EARTH-MOON SYSTEM, LUNAR SURFACE, METEORITE
                    COLLISIONS, MICROMETEOROIDS, HEOS SATELLITES,
                    METEOROID CONCENTRATION, METEOROID DUST CLOUDS,
                    POISSON DENSITY FUNCTIONS},
  BIBCODE       = {1977spar.meet..623D},
  ABSTRACT      = {
An analysis is performed which shows that the micrometeoroid groups
observed by the HEOS 2 satellite could have been produced by ejections
from the lunar surface during primary impacts by meteorites. A simplified
physical model is used to evaluate the distribution of secondary particles
resulting from the impact of a meteorite on the lunar surface. The number,
density, and approximate dimensions of clouds of secondary particles
produced by such an impact are estimated along with the expected number of
clouds intercepted by HEOS 2, the conditional probability that the
satellite sensor will encounter a certain number of particles belonging to
the same cloud, and the expected number of events. The results obtained
indicate that secondary ejecta from primary objects with a mass of 1 kg or
more constitute a possible origin for the particle groups observed by HEOS
2. It is noted that the calculated results are in order-of-magnitude
agreement with the observations.},
  topic         = {EARTH}
}

