@Article{         dermott-et-al-1990,
  title         = {Modelling the {IRAS} solar system dust bands},
  author        = {Dermott, S. F. and Nicholson, P. D. and Malhotra, R. and
                    Gomes, R. S.},
  AFFILIATION   = {AC(Cornell University, Ithaca, NY) AD(Observatorio
