@Inbook{          french-et-al-1991,
  title         = {Dynamics and structure of the {Uranian} rings},
  author        = {French, R. G. and Nicholson, P. D. and Porco, C. C. and
                  Marouf, E. A.},
  AFFILIATION   = {AA(Wellesley College, MA) AB(Cornell University,
                    Ithaca, NY) AC(Arizona, University, Tucson)
                    AD(Stanford University, CA)},
  booktitle     = {Uranus},
  publisher     = {Tucson, AZ, University of Arizona Press},
  year          = {1991},
  pages         = {327--409},
  KEYWORDS      = {PLANETARY ORBITS, PLANETOLOGY, URANUS RINGS,
                    DIFFRACTION PATTERNS, IMAGE ANALYSIS, ORBITAL
                    ELEMENTS, ORBITAL RESONANCES (CELESTIAL MECHANICS),
                    PARTICLE SIZE DISTRIBUTION, URANUS SATELLITES,
                    VOYAGER 2 SPACECRAFT},
  BIBCODE       = {1991uran.book..327F},
  ABSTRACT      = {
The current knowledge of the dynamics and structure of the Uranian
rings is reviewed, with an emphasis on the observations that have
allowed the determination of accurate ring orbits and detailed radial
structure. It is shown that several of the Uranian ring orbits have a
close connection with two small satellites, providing strong evidence
for ring confinement by shepherds. Some tentative conclusions
concerning particle size and the vertical structure of the rings are
drawn from a quantitative study of optical depth variations with
wavelength and orbital phase. Ring structure is examined; the rings
are found to be remarkably diverse in their radial structure and
azimuthal variations. The central elements of the dynamics of
shepherding as currently understood, and tempered by observational
constraints, is presented. A selection of open questions and
prospects for future progress is provided.},
  topic         = {URANUS}
}

