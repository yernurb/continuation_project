  title         = {Trapped radiation belts of {Saturn}~--- {First} look},
  author        = {Fillius, W. and Ip, W. H. and MacIlwain, C. E.},
  AFFILIATION   = {AA(California, University, La Jolla, Calif.)
                    AB(Max-Planck-Institut fuer Aeronomie, Katlenburg,
                    West Germany) AC(California, University, San Diego,
                    Calif.)},
  journal       = {Science},
  volume        = {207},
  date          = {Jan. 25},
  year          = {1980},
